<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="178.0" prefWidth="334.0" style="-fx-background-image: url(Images/background.jpg); -fx-background-size: 100% 100%;" stylesheets="@porpertiesStyle.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.PropertiesController">
    <top>
        <Label fx:id="label" BorderPane.alignment="CENTER" />
    </top>
    <bottom>
        <Button fx:id="btnUpdate" mnemonicParsing="false" onAction="#UpdateClicked" text="Update" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <BorderPane.margin>
                <Insets bottom="10.0" />
            </BorderPane.margin>
        </Button>
    </bottom>
    <left>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="107.0" text="Generator Type" textFill="WHITE" translateX="20.0" translateY="-20.0" wrapText="true" BorderPane.alignment="CENTER">
            <graphic>
                <ChoiceBox fx:id="generator" prefWidth="150.0" scaleX="0.7" translateY="50.0" />
            </graphic>
        </Label>
    </left>
    <right>
        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="117.0" text="Searching Algorithm" textFill="WHITE" translateX="-20.0" translateY="-20.0" wrapText="true" BorderPane.alignment="CENTER">
            <graphic>
                <ChoiceBox fx:id="searchingAlgorithm" prefWidth="150.0" scaleX="0.7" translateY="50.0" />
            </graphic>
        </Label>
    </right>
</BorderPane>
